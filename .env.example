# PalaCMS Environment Variables
# Copy this file to .env and customize the values

# Superuser credentials (for PocketBase admin access)
# These are optional and only used on first startup to create the initial admin user
PALA_SUPERUSER_EMAIL=admin@example.com
PALA_SUPERUSER_PASSWORD=changeme123

# PalaCMS user credentials
# These are optional and only used on first startup to create the initial PalaCMS user
PALA_USER_EMAIL=user@example.com
PALA_USER_PASSWORD=changeme123

# Version (optional)
PALA_VERSION=3.0.0-beta.1
